{
  "name": "alphavantage-stock-mcp",
  "version": "1.0.1",
  "description": "Stock data and analysis server using Alpha Vantage API for Model Context Protocol (MCP)",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "build_tsc": "tsc",
    "build:dev": "webpack --config webpack.config.js --mode development",
    "build:analyze": "webpack --config webpack.config.js --analyze",
    "type-check": "tsc --noEmit",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xweety/alphavantage-stock-mcp.git"
  },
  "homepage": "https://github.com/xweety/alphavantage-stock-mcp#readme",
  "keywords": [
    "stock",
    "stock-data",
    "investment",
    "stock-analyze",
    "alpha-vantage-api",
    "alpha-vantage-stock-data",
    "mcp",
    "model-context-protocol",
    "ai",
    "llm"
  ],
  "author": "Xweety & Godfrey Huang",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.18.2",
    "axios": "^1.12.2",
    "dotenv": "^16.3.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "copy-webpack-plugin": "^13.0.1",
    "eslint": "^9.37.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "terser-webpack-plugin": "^5.3.14",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "winston-daily-rotate-file": "^5.0.0"
  }
}
