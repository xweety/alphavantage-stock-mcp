{
  "name": "alphavantage-stock-mcp",
  "version": "1.0.5",
  "description": "MCP server for Stock data and analysis server using Alpha Vantage API",
  "main": "dist/index.js",
  "bin": {
    "alphavantage-stock-mcp": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --target=node18 --format=esm --outfile=dist/index.js --external:@modelcontextprotocol/sdk --packages=external --banner:js=\"#!/usr/bin/env node\"",
    "type-check": "tsc --noEmit",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xweety/alphavantage-stock-mcp.git"
  },
  "homepage": "https://github.com/xweety/alphavantage-stock-mcp#readme",
  "keywords": [
    "stock",
    "stock-data",
    "investment",
    "stock-analyze",
    "alpha-vantage-api",
    "alpha-vantage-stock-data",
    "mcp",
    "model-context-protocol",
    "ai",
    "llm"
  ],
  "author": "Xweety & Godfrey Huang",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.18.2",
    "axios": "^1.12.2",
    "dotenv": "^16.3.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "esbuild": "^0.19.12",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
